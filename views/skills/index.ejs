<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Dev Skills</title>
</head>
<body>
    <% skills.forEach(function(s, id){ %>
        <li>
            <a href="/skills/<%= s.id %>">
                <%= s.name %>
            </a>
        </li>
    <% }) %>
</body>
</html>